{
  "server": {
    "storagePath": "data",
    "cacheMaxMb": 1000
  },
  "connection": {
    "storageMethod": "file",
    "parallelism": 4,
    "cacheMaxMb": 200,
    "defaults": {
      "indexing": {
        "reindexMinBytes": 100000,
        "reindexMaxBytes": 10000000,
        "maxOldIndexes": 3
      }
    }
  },
  "consensus": {
    "protocol": "standalone",
    "maxPendingTxns": 42
  },
  "http": {
    "server": "jetty",
    "port": 8090,
    "maxTxnWaitMs": 120000
  },
  "profiles": {
    "dev": {
      "server": {
        "storagePath": "dev/data"
      },
      "http": {
        "port": 58090
      }
    }
  }
}
